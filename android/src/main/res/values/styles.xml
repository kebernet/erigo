<resources>

    <!-- Base application theme. -->
    <style id="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item id="colorPrimary">@color/colorPrimary</item>
        <item id="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item id="colorAccent">@color/colorAccent</item>
    </style>

    <style id="AppTheme.NoActionBar">
        <item id="windowActionBar">false</item>
        <item id="windowNoTitle">true</item>
    </style>

    <style id="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style id="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>
